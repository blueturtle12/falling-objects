{
  "extends": ["eslint:recommended", "prettier", "last"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "eqeqeq": ["error", "always"]
  },
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  }
}
